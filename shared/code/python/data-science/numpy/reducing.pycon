>>> a_3d = np.array([[[1, 2], [3, 4]], [[5, 6], [7, 8]]]); a_3d
array([[[1, 2],
        [3, 4]],

       [[5, 6],
        [7, 8]]])

>>> a_3d.mean(axis=0)
array([[3., 4.],
       [5., 6.]])

>>> a_3d.mean(axis=1)
array([[2., 3.],
       [6., 7.]])

>>> a_3d.mean(axis=2)
array([[1.5, 3.5],
       [5.5, 7.5]])

>>> a_3d.mean(axis=(0, 1))
array([4., 5.])
