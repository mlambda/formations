debug := \
    cnn-2018-09-22.pdf \


debugOff := \
    python-plot-data.pdf \
    chaine_markov_greedy.pdf \
    chaine_markov.pdf \
    notebook.pdf \
    python-plot-data-2019-01-22.pdf \
    python-plot-data.pdf \
    talk.pdf \
    gan-formules.pdf \

debug.pdf: $(debug)
	pdftk $+ cat output $@

all.pdf: $(all)
	pdftk $+ cat output $@

move: 
	echo $(all) $(debug) $(debugOff) | sed "s/ /\n/g" | sed "s/pdf/tex/"

%.pdf: %.tex formation.cls
	latexmk $<

clean:
	rm -rf *.aux *.fdb_latexmk *.fls *.log *.nav *.out *.snm *.toc *.vrb *.vrb~ _minted* all.pdf 

clean-pdfs: clean
	rm -rf *.pdf

.PHONY: clean
